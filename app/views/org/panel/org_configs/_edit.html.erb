<div class="ui modal" id="modal">
  <i class="fas fa-times close icon"></i>
  <div class="ui header blue">
    <%= t('.edit') %>
  </div>

  <div class="content">
    <%= form_with model: [:panel, @org_config] do |f| %>
      <%= render 'shared/error_messages', target: @org_config %>
      <%= f.collection_select :job_title_ids, JobTitle.default_where(default_params), :id, :name, {}, { multiple: true } %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
