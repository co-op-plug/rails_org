<%= form_with model: @organ, url: my_organs_path do |f| %>
  <%= render 'shared/error_messages', target: @organ %>
  <%= f.text_field :parent_uuid %>
  <%= f.text_field :name, required: true %>
  <%= f.file_field :logo %>
  <%= f.fields :members do |ef| %>
    <%= ef.select :identity, options_for_select(current_user.accounts.pluck(:identity, :identity)) %>
  <% end %>
  <%= f.submit %>
<% end %>
